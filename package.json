{
	"name": "echojs-lib",
	"version": "1.0.1",
	"description": "Pure JavaScript ECHO library for node.js",
	"main": "./dist/index.js",
	"types": "./types/index.d.ts",
	"scripts": {
		"postinstall": "npm run build",
		"test": "mocha --require babel-core/register --require babel-polyfill --exit",
		"test-api": "mocha --compilers js:babel-core/register --require babel-polyfill test/api.test.js",
		"test-subscriber": "mocha --compilers js:babel-core/register --require babel-polyfill test/subscriber.test.js",
		"build": "npx babel-cli src --out-dir dist",
		"prepublishOnly": "npm run build",
		"lint": "eslint .",
		"lint:debug": "npm run lint -- --debug",
		"lint:fix": "npm run lint -- --fix",
		"jsdoc2md": "jsdoc2md src/echo/api.js > api.md"
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.pixelplex.by:645.echo/echojs/echojs-lib.git"
	},
	"author": "PixelPlex",
	"license": "UNLICENSED",
	"dependencies": {
		"babel-core": "^6.26.3",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"assert": "^1.4.1",
		"babel-runtime": "^6.26.0",
		"bigi": "^1.4.2",
		"bignumber.js": "^8.0.1",
		"bs58": "^4.0.1",
		"buffer": "^5.2.1",
		"bytebuffer": "^5.0.1",
		"create-hash": "^1.2.0",
		"create-hmac": "^1.1.7",
		"crypto-js": "^3.1.9-1",
		"deep-equal": "^1.0.1",
		"ecurve": "^1.0.6",
		"events": "^3.0.0",
		"immutable": "^3.8.1",
		"isomorphic-ws": "^4.0.1",
		"lodash": "^4.17.11",
		"long": "^4.0.0",
		"tweetnacl": "^1.0.0",
		"ws": "^6.1.2"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-spies": "^1.0.0",
		"eslint": "^4.18.2",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "^2.9.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"mocha": "^5.2.0",
		"redux": "^4.0.1",
		"jsdoc-to-markdown": "^4.0.1"
	}
}
