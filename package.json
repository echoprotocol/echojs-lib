{
	"name": "echojs-lib",
	"version": "1.6.0-rc.2",
	"description": "Pure JavaScript ECHO library for node.js",
	"main": "./dist/index.js",
	"types": "./types/index.d.ts",
	"scripts": {
		"pretest": "babel-node ./test/preparation/preparation.js",
		"test": "mocha --require @babel/register --require @babel/polyfill --exit",
		"test-api": "mocha --compilers js:@babel/register --require @babel/polyfill test/api.test.js",
		"test-subscriber": "mocha --compilers js:@babel/register --require @babel/polyfill test/subscriber.test.js",
		"build": "npx babel src --out-dir dist",
		"lint": "eslint .",
		"lint:debug": "npm run lint -- --debug",
		"lint:fix": "npm run lint -- --fix"
	},
	"homepage": "https://github.com/echoprotocol/echojs-lib",
	"repository": {
		"type": "git",
		"url": "https://github.com/echoprotocol/echojs-lib.git"
	},
	"bugs": {
		"url": "https://github.com/echoprotocol/echojs-lib/issues"
	},
	"author": "PixelPlex",
	"license": "MIT",
	"dependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/polyfill": "^7.2.5",
		"@babel/runtime": "^7.3.1",
		"@types/node": "^12.0.10",
		"assert": "^1.4.1",
		"bigi": "^1.4.2",
		"bignumber.js": "^8.0.1",
		"bs58": "^4.0.1",
		"buffer": "^5.2.1",
